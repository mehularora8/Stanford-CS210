{
  "expo": {
    "name": "Parea",
    "slug": "Parea",
    "privacy": "unlisted",
    "version": "1.2.01",
    "platforms": [
      "ios"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "edu.stanford.cs210.parea",
      "infoPlist": {
        "NSLocationAlwaysAndWhenInUseUsageDescription": "Show your current location on the map",
        "NSLocationWhenInUseUsageDescription":"Let us use your location to help you find resources"
      }
    }
  }
}
