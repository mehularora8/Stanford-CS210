{
  "expo": {
    "name": "PareaReact",
    "slug": "PareaReact",
    "privacy": "unlisted",
    "version": "1.0.1",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "edu.stanford.parea",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription":"Let us use your location to help you find resources"
      }
    }, 
    "web": {
      "build": {
        "babel": {
          "include": ["galio-framework"]
        }
      }
    }
  }
}
