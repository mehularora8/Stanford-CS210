{
  "expo": {
    "name": "PareaReact",
    "slug": "PareaReact",
    "version": "1.0.0",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "io.expo.client"
    },
    "web": {
      "build": {
        "babel": {
          "include": [
            "galio-framework"
          ]
        }
      }
    },
    "android": {
      "package": "io.expo.client"
    }
  }
}
